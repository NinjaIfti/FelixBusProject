{
  "functions": {
    "pages/*.php": {
      "runtime": "vercel-php@0.6.0"
    }
  },
  "routes": [
    {
      "src": "/",
      "dest": "/pages/index.php"
    },
    {
      "src": "/([^/]+)",
      "dest": "/pages/$1.php"
    },
    {
      "src": "/([^/]+)\\.php",
      "dest": "/pages/$1.php"
    }
  ]
}
